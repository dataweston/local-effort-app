{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/search-images.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/_ping.js",
      "use": "@vercel/node"
    },
    {
      "src": "backend/api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "backend/api/ping.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/search-images",
      "dest": "/api/search-images.js"
    },
    {
      "src": "/api/_ping",
      "dest": "/api/_ping.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/server.js"
    },
  { "src": "^/$", "dest": "/prerender/index.html" },
  { "src": "^/about$", "dest": "/prerender/about/index.html" },
  { "src": "^/services$", "dest": "/prerender/services/index.html" },
  { "src": "^/pricing$", "dest": "/prerender/pricing/index.html" },
  { "src": "^/menu$", "dest": "/prerender/menu/index.html" },
  { "src": "^/happy-monday$", "dest": "/prerender/happy-monday/index.html" },
  { "src": "^/gallery$", "dest": "/prerender/gallery/index.html" },
  { "src": "^/meal-prep$", "dest": "/prerender/meal-prep/index.html" },
  { "src": "^/partner-portal$", "dest": "/prerender/partner-portal/index.html" },
    {
      "src": "/((?!api|assets|.*\\.|_next).*)",
      "dest": "/index.html"
    }
  ]
}
