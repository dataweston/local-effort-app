{
  "name": "local-effort-website",
  "version": "1.0.0",
  "private": true,
  "description": "Website for the Local Effort project.",
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.21.0",
    "@portabletext/react": "^4.0.1",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^7.9.0",
    "@sanity/image-url": "^1.1.0",
    "@vercel/analytics": "^1.5.0",
    "animejs": "^3.2.2",
    "aws-sdk": "^2.1692.0",
    "cloudinary": "^2.7.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "emailjs-com": "^3.2.0",
    "embla-carousel": "^8.1.6",
    "embla-carousel-react": "^8.1.6",
    "express": "^4.19.2",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.451.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-router": "^6.25.1",
    "react-router-dom": "^6.25.1",
    "web-push": "^3.6.6"
  },
  "overrides": {
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2"
  },
  "packageManager": "npm@10.8.1",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^24.3.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~4.7.4",
    "vite": "^5.1.7",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vite test",
    "eject": "vite eject",
    "lint": "eslint \"src/**/*.{js,jsx}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json,md}\"",
    "check-format": "prettier --check \"src/**/*.{js,jsx,css,json,md}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
